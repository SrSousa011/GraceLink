pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', 
                    url: 'https://github.com/SrSousa011/GraceLink.git'
            }
        }
        stage('Build and Test') {
            steps {
                sh 'flutter clean'
                sh 'flutter pub get'
                sh 'flutter test'
            }
        }
    }
}
